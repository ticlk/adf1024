{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "数据工厂名称",
			"defaultValue": "adf16"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "“AzureBlobStorage1”的“connectionString”的安全字符串"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "“AzureBlobStorage2”的“connectionString”的安全字符串"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "“AzureDataLakeStorage1”的“accountKey”的安全字符串"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "“AzureDataLakeStorage2”的“accountKey”的安全字符串"
		},
		"AzureDataLakeStorage3_accountKey": {
			"type": "secureString",
			"metadata": "“AzureDataLakeStorage3”的“accountKey”的安全字符串"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "“AzureSqlDatabase1”的“connectionString”的安全字符串"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "“AzureSynapseAnalytics1”的“connectionString”的安全字符串"
		},
		"AzureSynapseAnalytics2_connectionString": {
			"type": "secureString",
			"metadata": "“AzureSynapseAnalytics2”的“connectionString”的安全字符串"
		},
		"Sftp1_password": {
			"type": "secureString",
			"metadata": "“Sftp1”的“password”的安全字符串"
		},
		"linkedService1_password": {
			"type": "secureString",
			"metadata": "“linkedService1”的“password”的安全字符串"
		},
		"linkedService2_connectionString": {
			"type": "secureString",
			"metadata": "“linkedService2”的“connectionString”的安全字符串"
		},
		"storage1021_connectionString": {
			"type": "secureString",
			"metadata": "“storage1021”的“connectionString”的安全字符串"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage0601.dfs.core.chinacloudapi.cn/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage0601.dfs.core.chinacloudapi.cn/"
		},
		"AzureDataLakeStorage3_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage1107.dfs.core.chinacloudapi.cn/"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://[site-url]/_api/web/GetFileByServerRelativeUrl('[relative-path-to-file]')/$value"
		},
		"Sftp1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "10.0.0.18"
		},
		"Sftp1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "lihong"
		},
		"linkedService1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"sharepointHttpServer2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mcpbi.sharepoint.cn/sites/lhsite/_api/web/GetFileByServerRelativeUrl('/sites/lhsite/Shared%20Documents/test.xlsx')/$value"
		},
		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1f2ffc2d-4f6a-4fea-b142-31ae337d6aad/resourceGroups/NetworkWatcherRG/providers/Microsoft.Storage/storageAccounts/storage0601"
		},
		"AzureBlobStorage505_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/1f2ffc2d-4f6a-4fea-b142-31ae337d6aad/resourceGroups/NetworkWatcherRG/providers/Microsoft.Storage/storageAccounts/storage0601"
		},
		"AzureBlobStorage505_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"AzureBlobStorage505_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"storage0601.blob.core.chinacloudapi.cn"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "包含链接 ARM 模板的存储帐户的 URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "包含链接 ARM 模板的存储帐户的 SAS 令牌"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf16_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"AzureSynapseAnalytics2_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics2_connectionString')]"
					},
					"linkedService2_connectionString": {
						"value": "[parameters('linkedService2_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"sharepointHttpServer2_properties_typeProperties_url": {
						"value": "[parameters('sharepointHttpServer2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf16_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage3_accountKey": {
						"value": "[parameters('AzureDataLakeStorage3_accountKey')]"
					},
					"Sftp1_password": {
						"value": "[parameters('Sftp1_password')]"
					},
					"linkedService1_password": {
						"value": "[parameters('linkedService1_password')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"storage1021_connectionString": {
						"value": "[parameters('storage1021_connectionString')]"
					},
					"AzureDataLakeStorage3_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage3_properties_typeProperties_url')]"
					},
					"Sftp1_properties_typeProperties_host": {
						"value": "[parameters('Sftp1_properties_typeProperties_host')]"
					},
					"Sftp1_properties_typeProperties_userName": {
						"value": "[parameters('Sftp1_properties_typeProperties_userName')]"
					},
					"linkedService1_properties_typeProperties_userName": {
						"value": "[parameters('linkedService1_properties_typeProperties_userName')]"
					},
					"AzureBlobStorage505_properties_privateLinkResourceId": {
						"value": "[parameters('AzureBlobStorage505_properties_privateLinkResourceId')]"
					},
					"AzureBlobStorage505_properties_groupId": {
						"value": "[parameters('AzureBlobStorage505_properties_groupId')]"
					},
					"AzureBlobStorage505_properties_fqdns": {
						"value": "[parameters('AzureBlobStorage505_properties_fqdns')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf16_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf16_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger2_properties_typeProperties_scope": {
						"value": "[parameters('trigger2_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf16_ArmTemplate_1')]"
			]
		}
	]
}